{
  "plugins": {
    "commit-analyzer": {
      "name": "default@^1.0.0"
    },
    "ci-condition": {
      "name": "default"
    },
    "changelog-generator": {
      "name": "default",
      "options": {
        "emojis": "true"
      }
    },
    "provider": {
      "name": "gitlab",
      "options": {
        "gitlab_baseurl": "https://{{cookiecutter.gitlab_host}}",
        "gitlab_projectid": "{{cookiecutter.gitlab_projectid}}"
      }
    }
  }
}
